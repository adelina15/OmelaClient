<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/flowersListFragment">

    <fragment
        android:id="@+id/flowersListFragment"
        android:name="com.example.omela.main.FlowersListFragment"
        android:label="FlowersListFragment"
        tools:layout="@layout/fragment_flowers_list">
        <action
            android:id="@+id/action_flowersListFragment_to_filterFragment"
            app:destination="@id/filterFragment" />
        <action
            android:id="@+id/action_flowersListFragment_to_authorFragment"
            app:destination="@id/authorFragment" />
        <action
            android:id="@+id/action_flowersListFragment_to_flowerDetailsFragment"
            app:destination="@id/flowerDetailsFragment" />
        <action
            android:id="@+id/action_flowersListFragment_to_categorieFragment"
            app:destination="@id/categorieFragment" />
    </fragment>
    <fragment
        android:id="@+id/filterFragment"
        android:name="com.example.omela.main.FilterFragment"
        android:label="FilterFragment" >
        <action
            android:id="@+id/action_filterFragment_to_flowersListFragment"
            app:destination="@id/flowersListFragment" />
        <action
            android:id="@+id/action_filterFragment_to_authorFragment"
            app:destination="@id/authorFragment" />
    </fragment>
    <fragment
        android:id="@+id/authorFragment"
        android:name="com.example.omela.AuthorFragment"
        android:label="fragment_author"
        tools:layout="@layout/fragment_author" >
        <action
            android:id="@+id/action_authorFragment_to_flowersListFragment"
            app:destination="@id/flowersListFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragment"
        android:name="com.example.omela.FavoritesFragment"
        android:label="FavoritesFragment" >
        <action
            android:id="@+id/action_favoritesFragment_to_flowerDetailsFragment"
            app:destination="@id/flowerDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/basketFragment"
        android:name="com.example.omela.basket.BasketFragment"
        android:label="BasketFragment" >
        <action
            android:id="@+id/action_basketFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_basketFragment_to_statusFragment"
            app:destination="@id/statusFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.omela.account.AccountFragment"
        android:label="AccountFragment" >
        <action
            android:id="@+id/action_accountFragment_to_branchesFragment"
            app:destination="@id/branchesFragment" />
        <action
            android:id="@+id/action_accountFragment_to_orderHistoryFragment"
            app:destination="@id/orderHistoryFragment" />
        <action
            android:id="@+id/action_accountFragment_to_editAccountFragment"
            app:destination="@id/editAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/branchesFragment"
        android:name="com.example.omela.account.BranchesFragment"
        android:label="BranchesFragment" >
        <action
            android:id="@+id/action_branchesFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderHistoryFragment"
        android:name="com.example.omela.account.OrderHistoryFragment"
        android:label="OrderHistoryFragment" >
        <action
            android:id="@+id/action_orderHistoryFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_orderHistoryFragment_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderFragment"
        android:name="com.example.omela.basket.OrderFragment"
        android:label="OrderFragment" >
        <action
            android:id="@+id/action_orderFragment_to_statusFragment"
            app:destination="@id/statusFragment" />
        <action
            android:id="@+id/action_orderFragment_to_basketFragment"
            app:destination="@id/basketFragment" />
    </fragment>
    <fragment
        android:id="@+id/statusFragment"
        android:name="com.example.omela.basket.StatusFragment"
        android:label="StatusFragment" >
        <action
            android:id="@+id/action_statusFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
    </fragment>
    <fragment
        android:id="@+id/editAccountFragment"
        android:name="com.example.omela.account.EditAccountFragment"
        android:label="EditAccountFragment" >
        <action
            android:id="@+id/action_editAccountFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/flowerDetailsFragment"
        android:name="com.example.omela.main.FlowerDetailsFragment"
        android:label="FlowerDetailsFragment" >
        <argument
            android:name="flower"
            app:argType="com.example.omela.model.FlowersItem" />
        <action
            android:id="@+id/action_flowerDetailsFragment_to_flowersListFragment"
            app:destination="@id/flowersListFragment" />
    </fragment>
    <fragment
        android:id="@+id/categorieFragment"
        android:name="com.example.omela.main.CategoryFragment"
        android:label="CategorieFragment" >
        <action
            android:id="@+id/action_categorieFragment_to_flowersListFragment"
            app:destination="@id/flowersListFragment" />
        <argument
            android:name="category_name"
            app:argType="string" />
        <action
            android:id="@+id/action_categorieFragment_to_flowerDetailsFragment"
            app:destination="@id/flowerDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.example.omela.account.OrderDetailsFragment"
        android:label="OrderDetailsFragment" >
        <action
            android:id="@+id/action_orderDetailsFragment_to_orderHistoryFragment"
            app:destination="@id/orderHistoryFragment" />
    </fragment>
</navigation>